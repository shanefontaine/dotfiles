{
    // ---------- Editor ----------
    "files.autoSave": "onWindowChange",
    "editor.wordWrapColumn": 88,
    "editor.renderWhitespace": "all",
    "editor.bracketPairColorization.enabled": true,
    // Do not autocomplete after clicking '.'
    "editor.acceptSuggestionOnCommitCharacter": false,
    "workbench.editor.tabSizing": "shrink",
    "git.confirmSync": false,
    "files.exclude": {
      "**/*.js: { \"when\": \"$(basename).ts\" }": true
    },
    "explorer.autoReveal": false,
    "workbench.colorTheme": "Winter is Coming (Dark Blue)",
    "diffEditor.ignoreTrimWhitespace": false,
    "files.associations": {
      "*.ts": "typescript"
    },

    "workbench.sideBar.location": "left",
    "editor.minimap.enabled": false,
    "workbench.activityBar.visible": false,
    "peacock.favoriteColors": [
      {
        "name": "Angular Red",
        "value": "#b52e31"
      },
      {
        "name": "Auth0 Orange",
        "value": "#eb5424"
      },
      {
        "name": "Azure Blue",
        "value": "#007fff"
      },
      {
        "name": "C# Purple",
        "value": "#68217A"
      },
      {
        "name": "Gatsby Purple",
        "value": "#639"
      },
      {
        "name": "Go Cyan",
        "value": "#5dc9e2"
      },
      {
        "name": "Java Blue-Gray",
        "value": "#557c9b"
      },
      {
        "name": "JavaScript Yellow",
        "value": "#f9e64f"
      },
      {
        "name": "Mandalorian Blue",
        "value": "#1857a4"
      },
      {
        "name": "Node Green",
        "value": "#215732"
      },
      {
        "name": "React Blue",
        "value": "#00b3e6"
      },
      {
        "name": "Something Different",
        "value": "#832561"
      },
      {
        "name": "Vue Green",
        "value": "#42b883"
      }
    ],
    "peacock.affectAccentBorders": true,
    "peacock.surpriseMeOnStartup": true,
    "editor.fontLigatures": true,
    "editor.fontFamily": "'Cascadia Code', Consolas, 'Courier New', monospacesett",
    // Add custom comment colors
    "better-comments.tags": [
      {
        "tag": "audit",
        "color": "#FF0000",
        "strikethrough": false,
        "backgroundColor": "transparent"
      },
      {
        "tag": "note",
        "color": "#3498DB",
        "strikethrough": false,
        "backgroundColor": "transparent"
      }
    ],

    // ---------- Languages ----------
    "[javascript]": {
      "editor.rulers": [
        80
      ],
      "editor.wordWrapColumn": 80,
      "editor.tabSize": 2,
      "editor.insertSpaces": true,
      "editor.detectIndentation": false,
      // Get rid of assert autocomplete
      "editor.acceptSuggestionOnCommitCharacter": false
    },
    "[typescript]": {
      "editor.rulers": [
        120
      ],
      "editor.wordWrapColumn": 120,
      "editor.tabSize": 2,
      "editor.insertSpaces": true,
      "editor.detectIndentation": false
    },

    "[solidity]": {
      "editor.rulers": [
        145
      ],
      "editor.wordWrapColumn": 145,
      "editor.tabSize": 4,
      "editor.insertSpaces": true,
      "editor.detectIndentation": false
    },
    // Solium rule disables
    "solidity.soliumRules": {
      "security/no-block-members": "off",
      "security/no-low-level-calls": "off",
      "security/no-tx-origin": "off",
      "security/no-inline-assembly": "off",
      "error-reason": "false"
    },

    // Remove autoimport
    "typescript.updateImportsOnFileMove.enabled": "never",
    "typescript.suggest.autoImports": false,
    "javascript.suggest.autoImports": false,

    // ---------- git ----------

    // Remove git commit message line length
    "git.inputValidation": "off",

    // ---------- Vim ----------

    // ---------- Potential fix for vim plugin lag ----------
    // https://github.com/VSCodeVim/Vim/issues/3957#issuecomment-518581270
    "vim.statusBarColorControl": false,
    //  https://github.com/VSCodeVim/Vim/issues/3957#issuecomment-592584199
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": [
          "j"
        ],
        "commands": [
          "cursorDown"
        ]
      },
      {
        "before": [
          "k"
        ],
        "commands": [
          "cursorUp"
        ]
      }
    ],
    // https://github.com/VSCodeVim/Vim/issues/3957#issuecomment-592585035
    // Fix for VIM plugin copy/paste not working
    // https://stackoverflow.com/questions/58306002/vs-code-vim-extension-copy-and-paste
    "vim.handleKeys": {
      "<C-c>": false,
      "<C-v>": false
    },


